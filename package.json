    "bootstrap-sass": "^3.3.5",
    "bootstrap-sass-loader": "^1.0.9",
    "classnames": "^2.1.3",
    "css-loader": "^0.19.0",
    "eslint": "^1.7.3",
    "eslint-config-rackt": "^1.1.0",
